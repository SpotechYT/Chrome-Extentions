{
    "manifest_version": 2,
    "name": "Ez Media",
    "description": "A extention that allows you to get to your favorite websites quickly", 
    "version": "1.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"  
    },
"permissions": ["activeTab"]
}